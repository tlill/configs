set nocompatible
filetype indent plugin on

syntax on

" menu stuff
set wildmenu
set showcmd

" search stuff
set hlsearch
set ignorecase
set smartcase

set autoindent

set nostartofline

set confirm
set visualbell

set mouse=a

set nu rnu
set hidden
set showcmd
set backspace=indent,eol,start

nnoremap <C-L> :nohl<CR><C-L>

